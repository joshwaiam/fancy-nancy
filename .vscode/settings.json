{
  "eslint.workingDirectories": [
    {
      "directory": "./",
      "changeProcessCWD": true
    }
  ],
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    {
      "autoFix": true,
      "language": "typescript"
    }
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "prettier-eslint.eslintIntegration": true
}
